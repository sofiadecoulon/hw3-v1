<h1><%= @place["name"] %></h1>


<h2>Entries:</h2>

<ul>
  <% for entry in @entry %>
    <h3><%= @entry["title"] %></h3>
    <p><%= @entry["description"] %></p>
     <p><%= @entry["occurred_on"] %></p>
  <% end %>
</ul>

<p>
  <a href="/entry/new?id=<%= @place["id"] %>">New Entry</a>
</p>

<p>
  <a href="/places">Back to All Places</a>
</p>